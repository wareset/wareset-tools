{
  "name": "@wareset-tools/rease-pixi-legacy",
  "version": "1.0.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "wip",
  "keywords": [
    "rease-pixi-legacy"
  ],
  "license": "MIT",
  "author": "rease <cftcview@gmail.com>",
  "main": "index",
  "module": "index.mjs",
  "types": "types/index.d.ts",
  "files": [
    "README.md",
    "_includes/**/*",
    "index.d.ts",
    "index.js",
    "index.mjs",
    "pixi/**/*",
    "types/**/*"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./index.mjs",
      "require": "./index.js",
      "types": "./types/index.d.ts"
    },
    "./pixi": {
      "import": "./pixi/index.mjs",
      "require": "./pixi/index.js",
      "types": "./types/pixi.d.ts"
    }
  },
  "scripts": {
    "build": "dester -m",
    "dev": "dester -mw"
  },
  "peerDependencies": {
    "rease": "0.4.x"
  },
  "devDependencies": {
    "dester": "1.0.x",
    "typescript": "5.7.2"
  },
  "dependencies": {
    "@pixi/graphics-extras-legacy": "npm:@pixi/graphics-extras@7.4.2",
    "@pixi/sound-legacy": "npm:@pixi/sound@5.2.3",
    "pixi.js-legacy": "7.4.2"
  }
}